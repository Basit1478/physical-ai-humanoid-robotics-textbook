{
  "name": "physical-ai-humanoid-robotics-backend",
  "version": "1.0.0",
  "description": "Backend for the Physical AI & Humanoid Robotics Textbook with RAG, authentication, personalization, and translation",
  "main": "index.js",
  "scripts": {
    "dev": "uvicorn app.main:app --reload --host 0.0.0.0 --port 8000",
    "start": "uvicorn app.main:app --host 0.0.0.0 --port 8000",
    "migrate": "alembic upgrade head",
    "test": "pytest",
    "install": "pip install -r requirements.txt",
    "docs": "python -m pydoc.main"
  },
  "keywords": [
    "ai",
    "robotics",
    "textbook",
    "rag",
    "fastapi",
    "nlp",
    "translation"
  ],
  "author": "Physical AI & Humanoid Robotics Project",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}